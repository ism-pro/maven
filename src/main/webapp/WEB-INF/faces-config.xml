<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>fr</default-locale>
            <supported-locale>fr_BE</supported-locale>
            <supported-locale>fr</supported-locale>
        </locale-config>
        
        <!-- Config. for adding font-awesome -->
        <!--
        <resource-handler>
            org.omnifaces.resourcehandler.UnmappedResourceHandler
        </resource-handler>
        -->
        
        
        <!-- Developpement only
         <lifecycle>
             <phase-listener>org.ism.listener.LogPhaseListener</phase-listener>
         </lifecycle>
        -->
        
        
        <resource-bundle>
            <base-name>com.ISM</base-name>
            <var>ism</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/META-INF/Security</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleMailType</base-name>
            <var>bundleMailType</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
   
    
     
      
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
    <name>ism</name>
    
    <component>
        <component-type>org.primefaces.ism.component.DataTable</component-type>
        <component-class>org.primefaces.ism.component.datatable.DataTable</component-class>
    </component>
    
    <component>
        <component-type>org.primefaces.ism.component.Table</component-type>
        <component-class>org.primefaces.ism.component.table.Table</component-class>
    </component>

    <component>
        <component-type>org.primefaces.ism.component.OverlayPanel</component-type>
        <component-class>org.primefaces.ism.component.overlaypanel.OverlayPanel</component-class>
    </component>
    
    <component>
        <component-type>org.primefaces.ism.component.Ribbon</component-type>
        <component-class>org.primefaces.ism.component.ribbon.Ribbon</component-class>
    </component>

    <component>
        <component-type>org.primefaces.ism.component.RibbonGroup</component-type>
        <component-class>org.primefaces.ism.component.ribbon.RibbonGroup</component-class>
    </component>
        
    <component>
        <component-type>org.primefaces.ism.component.RibbonGroupSet</component-type>
        <component-class>org.primefaces.ism.component.ribbon.RibbonGroupSet</component-class>
    </component>


    <render-kit>     
        
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.DataTableRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.datatable.DataTableRenderer</renderer-class>
        </renderer>
        
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.TableRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.table.TableRenderer</renderer-class>
        </renderer>
        
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.OverlayPanelRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.overlaypanel.OverlayPanelRenderer</renderer-class>
        </renderer>
        
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.RibbonRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.ribbon.RibbonRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.RibbonGroupRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.ribbon.RibbonGroupRenderer</renderer-class>
        </renderer>
        <renderer>
            <component-family>org.primefaces.ism.component</component-family>
            <renderer-type>org.primefaces.ism.component.RibbonGroupSetRenderer</renderer-type>
            <renderer-class>org.primefaces.ism.component.ribbon.RibbonGroupSetRenderer</renderer-class>
        </renderer>
    </render-kit>
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
</faces-config>
